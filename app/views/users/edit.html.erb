<div class="content">
  <% if notice %>
    <div class="alert alert-success" role="alert">
      <%= notice %>
    </div>
  <% end %>
  <%= render 'form' %>
  <% if @from == 'dashboard' %>
    <%= link_to dashboard_path do %>
      <button type="submit" class="btn btn-default mt20" aria-label="Left Align">
        <%= t('dic.back')%>
      </button>
    <% end %>
  <% else %>
    <%= link_to @user do %>
      <button type="submit" class="btn btn-default mt20" aria-label="Left Align">
        <%= t('dic.back')%>
      </button>
    <% end %>
  <% end %>
</div>
